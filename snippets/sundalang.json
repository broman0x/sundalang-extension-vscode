{
    "Print Statement": {
        "prefix": "cetak",
        "body": [
            "cetakkeun(\"$1\")"
        ],
        "description": "Cetak ke layar"
    },
    "Ask Input": {
        "prefix": "tanya",
        "body": [
            "tanyakeun(\"$1\")"
        ],
        "description": "Tanya input dari user"
    },
    "Variable Declaration": {
        "prefix": "tanda",
        "body": [
            "tanda ${1:nama} = ${2:nilai}"
        ],
        "description": "Deklarasi variabel"
    },
    "If Statement": {
        "prefix": "lamun",
        "body": [
            "lamun ${1:kondisi} {",
            "\t$0",
            "}"
        ],
        "description": "Pernyataan lamun (if)"
    },
    "If-Else Statement": {
        "prefix": "lamunteu",
        "body": [
            "lamun ${1:kondisi} {",
            "\t$2",
            "} lamunteu {",
            "\t$0",
            "}"
        ],
        "description": "Pernyataan lamun-lamunteu (if-else)"
    },
    "While Loop": {
        "prefix": "kedap",
        "body": [
            "kedap ${1:kondisi} {",
            "\t$0",
            "}"
        ],
        "description": "Loop kedap (while)"
    },
    "Function Definition": {
        "prefix": "fungsi",
        "body": [
            "fungsi ${1:nama}(${2:params}) {",
            "\t$0",
            "\tbalik ${3:nilai}",
            "}"
        ],
        "description": "Definisi fungsi"
    },
    "Return Statement": {
        "prefix": "balik",
        "body": [
            "balik $0"
        ],
        "description": "Return value dari fungsi"
    },
    "Boolean True": {
        "prefix": "bener",
        "body": [
            "bener"
        ],
        "description": "Nilai boolean true"
    },
    "Boolean False": {
        "prefix": "salah",
        "body": [
            "salah"
        ],
        "description": "Nilai boolean false"
    },
    "Comment": {
        "prefix": "#",
        "body": [
            "# $0"
        ],
        "description": "Komentar"
    },
    "Array Declaration": {
        "prefix": "array",
        "body": [
            "tanda ${1:nama} = [${2:item1}, ${3:item2}]"
        ],
        "description": "Deklarasi array"
    },
    "Hash/Map Declaration": {
        "prefix": "hash",
        "body": [
            "tanda ${1:nama} = {",
            "\t\"${2:key}\": ${3:value}",
            "}"
        ],
        "description": "Deklarasi hash/map"
    },
    "Sleep/Delay": {
        "prefix": "sare",
        "body": [
            "sare(${1:1000})"
        ],
        "description": "Sleep/delay dalam milidetik"
    },
    "Delay (alias)": {
        "prefix": "reureuh",
        "body": [
            "reureuh(${1:1000})"
        ],
        "description": "Delay/tunggu dalam milidetik"
    },
    "Array Length": {
        "prefix": "panjang",
        "body": [
            "panjang(${1:array})"
        ],
        "description": "Panjang array atau string"
    },
    "Array First": {
        "prefix": "mimiti",
        "body": [
            "mimiti(${1:array})"
        ],
        "description": "Elemen pertama array"
    },
    "Array Last": {
        "prefix": "tungtung",
        "body": [
            "tungtung(${1:array})"
        ],
        "description": "Elemen terakhir array"
    },
    "Array Push": {
        "prefix": "asupkeun",
        "body": [
            "asupkeun(${1:array}, ${2:value})"
        ],
        "description": "Tambahkan elemen ke array"
    },
    "String to Upper": {
        "prefix": "garede",
        "body": [
            "garede(${1:string})"
        ],
        "description": "Ubah string jadi huruf gede"
    },
    "String to Lower": {
        "prefix": "laleutik",
        "body": [
            "laleutik(${1:string})"
        ],
        "description": "Ubah string jadi huruf leutik"
    },
    "String to Number": {
        "prefix": "kana_angka",
        "body": [
            "kana_angka(${1:string})"
        ],
        "description": "Konversi string ka angka"
    },
    "To String": {
        "prefix": "kana_tulisan",
        "body": [
            "kana_tulisan(${1:value})"
        ],
        "description": "Konversi value ka string"
    },
    "Get Type": {
        "prefix": "tipe",
        "body": [
            "tipe(${1:value})"
        ],
        "description": "Cek tipe data"
    },
    "Main Program Template": {
        "prefix": "main",
        "body": [
            "# Program: ${1:Nama Program}",
            "",
            "cetakkeun(\"$2\")",
            "$0"
        ],
        "description": "Template program utama"
    },
    "Hello World": {
        "prefix": "hello",
        "body": [
            "cetakkeun(\"Halo Dunya!\")"
        ],
        "description": "Hello World program"
    }
}